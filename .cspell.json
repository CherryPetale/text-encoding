{
    "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/master/cspell.schema.json",
    "version": "0.2",
    "files": [
        "**/*"
    ],
    "enabledLanguageIds": [
        "markdown",
        "typescript"
    ],
    "ignorePaths": [
        "node_modules",
        "coverage",
        "data/**",
        "doc/api/**",
        "src/sandbox/**",
        "src/test/data/**",
        "package-lock.json",
        "git",
        "lib",
        "dist",
        "*.log",
        "*.tgz"
    ],
    "words": [
        "asmo",
        "CDEFGH",
        "csbig",
        "cseuckr",
        "cseucpkdfmtjapanese",
        "csgb",
        "csibm",
        "csiso",
        "csisolatin",
        "csisolatinarabic",
        "csisolatincyrillic",
        "csisolatingreek",
        "csisolatinhebrew",
        "cskoi",
        "csksc",
        "csmacintosh",
        "csshiftjis",
        "datastream",
        "elot",
        "eucjp",
        "euckr",
        "hkscs",
        "kayahr",
        "ponyfill",
        "Reimer",
        "sjis",
        "substeps",
        "SUPERSETS",
        "typedoc"
    ],
    "language": "en"
}
